entity News {
  title String,
  source String,
  link String required unique,
  kind String,
  time Instant,
  content String
}

entity UserExt{
	pthone String,
}
relationship OneToOne{
	 UserExt{user(email) } to User with jpaDerivedIdentifier
}

relationship ManyToMany{
	UserExt{news(title) } to News{userExt}
}

service News with serviceImpl

paginate News with pagination
